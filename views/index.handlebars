{{#each articles}}
<form action="/save" method="post">
	<h5 class="card-title">{{this.title}}</h5>
    	<h6>{{this.link}}</h6>
    	<br>
    	<button type="submit" href= "/save" class="btn btn-info">Save Article</button>
        <button type="submit" href= "/note" class="btn btn-info">Comment</button>
</form>
    	<hr>
{{/each}}